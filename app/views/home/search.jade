extends ../layout

block content
	h1 Result here.

	for resultItem in resultItems
		div.singleresult
			img(src=resultItem.img_url, style='width: inherit; height: inherit; z-index:-1; position: absolute; clip: rect(0px, 200px, 200px, 0px);')
			a(href=resultItem.url)
				h3 #{resultItem.title}
			//- p.hide #{resultItem.mbid}
			p.hide #{resultItem.type}
			p #{resultItem.artist_name}
			